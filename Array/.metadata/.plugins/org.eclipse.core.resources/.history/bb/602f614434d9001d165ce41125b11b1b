
public class Practice
{

	public static void main(String[] args) 
	{
		int []arr = new int [5];// declaration & instantiation
		
		arr[0]=10;
		arr[1]=20;
		arr[2]=30;// array initialization
		arr[4]=40;
		arr[5]=50;
		
		for(int i=0; i<arr.length;i++)
		{
			System.out.println(arr[i]);
		}
		
		
		

	}

}
